<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark  bg-dark border-bottom ">
            <div class="row">
                @*<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Factory Performance Booster</a>*@
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />                   
                </div>
            </div>
        </nav>
    </header>
    

    <div class="wrapper d-flex align-items-stretch">
        <nav id="sidebar">
            <div class="custom-menu">
                <button type="button" id="sidebarCollapse" class="btn btn-primary" >
                    <i class="fa fa-bars"></i>
                </button>
            </div>

            <div class="p-4">
                <h1><a class="logo" asp-controller="Home" asp-action="Empresas">Factory <span>Performance Booster</span></a></h1>

                <ul class="list-unstyled components mb-5">
                    <li>
                        <a asp-action="VistaPrincipal" asp-controller="Home" asp-route-idEmpresa="@ViewData["idEmpresa"]"><i class="fas fa-home"></i> Inicio</a>
                    </li>
                    <li>
                        <a asp-area="Identity" asp-page="/Account/Manage/Index" asp-route-idEmpresa="@ViewData["idEmpresa"]"><i class="fas fa-user"></i> Administración de Cuenta</a>
                    </li>

                    <li>
                        <a asp-area="Identity" asp-page="/Account/Manage/ChangePassword" asp-route-idEmpresa="@ViewData["idEmpresa"]"><i class="fas fa-key"></i> Cambio de contraseña</a>
                    </li>


                </ul>

                <div class="footer">
                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Copyright &copy;
                        <script>document.write(new Date().getFullYear());</script> | Factory Performance Booster <i class="icon-heart" aria-hidden="true"></i> by <a style="color:white" href="https://www.cogcs.com/" target="_blank">COGNITIVA</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>
        </nav>

        <!-- Page Content  -->
        <div id="content" class="p-4 p-md-5 pt-5">
            <div class="container">
                <partial name="_CookieConsentPartial" />
                <main role="main" class="pb-3" id="cuerpoPrincipal">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>
       

